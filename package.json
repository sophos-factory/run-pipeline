{
  "name": "run-pipeline",
  "version": "2.0.0",
  "scripts": {
    "build": "ncc build src/index.ts --minify --out dist/",
    "lint:fix": "eslint --fix src src/__tests__",
    "lint": "eslint src src/__tests__",
    "test": "jest",
    "test:watch": "jest -w",
    "pre-commit": "run-p build test lint:fix"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit && git add src/ dist/"
    }
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^5.0.3",
    "@sophos-factory/api-client": "^2.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^4.3.6",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.4"
  }
}
